<%= form_for([:admin, @home], :html => {:class => "form-horizontal"}) do |f| %>
  <% if @home.errors.any? %>
    <%= render partial: "shared/form_errors", :locals => {:errors => @home.errors} %>
  <% end %>
  <fieldset>
  <div class="field">
    <div class="form-group">
      <%= f.label :title, :class => "col-md-2 control-label" %>
      <div class="col-md-4">
        <%= f.text_field :title, :class => "form-control input-md" %>
      </div>
    </div>
  </div>
  <div class="field">
    <div class="form-group">
      <%= f.label :video_url, :class => "col-md-2 control-label" %>
      <div class="col-md-4">
        <%= f.url_field :video_url, :class => "form-control input-md" %>
      </div>
    </div>
  </div>
  <div class="field">
    <div class="form-group">
      <%= f.label :phone_number, :class => "col-md-2 control-label" %>
      <div class="col-md-4">
        <%= f.text_field :phone_number, :class => "form-control input-md" %>
      </div>
    </div>
  </div>
  <div class="field">
    <div class="form-group">
      <%= f.label :address, :class => "col-md-2 control-label" %>
      <div class="col-md-4">
        <%= f.text_field :address, :class => "form-control input-md" %>
      </div>
    </div>
  </div>
  <div class="field">
    <div class="form-group">
      <%= f.label :city, :class => "col-md-2 control-label" %>
      <div class="col-md-4">
        <%= f.text_field :city, :class => "form-control input-md" %>
      </div>
    </div>
  </div>
  <div class="field">
    <div class="form-group">
      <%= f.label :state, :class => "col-md-2 control-label" %>
      <div class="col-md-4">
        <%= f.text_field :state, :class => "form-control input-md" %>
      </div>
    </div>
  </div>
  <div class="field">
    <div class="form-group">
      <%= f.label :website, :class => "col-md-2 control-label" %>
      <div class="col-md-4">
        <%= f.url_field :website, :class => "form-control input-md" %>
      </div>
    </div>
  </div>
  <div class="field">
    <div class="form-group">
      <%= f.label :home_page_desc, :class => "col-md-2 control-label" %>
      <div class="col-md-4">
        <%= f.text_field :home_page_desc, :class => "form-control input-md" %>
      </div>
    </div>
  </div>
  <div class="field">
    <div class="form-group">
      <%= f.label :search_page_description, :class => "col-md-2 control-label" %>
      <div class="col-md-4">
        <%= f.text_area :search_page_description, :class => "form-control input-md" %>
      </div>
    </div>
  </div>
  <div class="field">
    <div class="form-group">
      <%= f.label :description, :class => "col-md-2 control-label" %>
      <div class="col-md-4">
        <%= f.text_area :description, :class => "form-control input-md" %>
      </div>
    </div>
  </div>
  <div class="field">
    <div class="form-group">
      <%= f.label :latitude, :class => "col-md-2 control-label" %>
      <div class="col-md-4">
        <%= f.text_field :latitude, :class => "form-control input-md" %>
      </div>
    </div>
  </div>
  <div class="field">
    <div class="form-group">
      <%= f.label :longitude, :class => "col-md-2 control-label" %>
      <div class="col-md-4">
        <%= f.text_field :longitude, :class => "form-control input-md" %>
      </div>
    </div>
  </div>
  <div class="field">
    <div class="form-group">
      <%= f.label :image, :class => "col-md-2 control-label" %>
      <div class="col-md-4">
        <%= f.file_field :image, :class => "form-control input-md" %>
      </div>
    </div>
  </div>
  <div class="field">
    <div class="form-group">
      <%= f.label :image_description, :class => "col-md-2 control-label" %>
      <div class="col-md-4">
        <%= f.text_field :image_description, :class => "form-control input-md" %>
      </div>
    </div>
  </div>
    <div class="form-group">
      <label class="col-md-2 control-label" for="singlebutton"></label>
      <div class="col-md-4">
        <%= f.submit :class => "btn btn-primary" %>
      </div>
    </div>
  </fieldset>
<% end %>
