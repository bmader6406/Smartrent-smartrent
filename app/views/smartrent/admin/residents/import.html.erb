<% content_for(:ui_center) do %>
  <div class="main">
    <%= form_tag import_admin_residents_path, :multipart => true, :class => "form-horizontal" do %>
      <fieldset>
        <legend>Import residents from a CSV file</legend>
        <div class="form-group">
          <%= label_tag :file, nil, class: "col-md-4 control-label" %>
          <div class="col-md-7">
            <%= file_field_tag :file %>
          </div>
        </div>
        <div class="form-group">
          <label class="col-md-4 control-label" for="singlebutton"></label>
          <div class="col-md-7">
            <%= submit_tag "Import", :class => "btn btn-primary" %>
          </div>
        </div>
      </fieldset>
    <% end %>
  </div>
<% end %>


<script type="text/javascript">
  $(function(){
    App.initPageLayout(null, null, null, true);
  });
</script>