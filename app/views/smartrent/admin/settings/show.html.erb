<% content_for(:ui_center) do %>
  <div class="main">
    <p>
      <b>Key:</b>
      <%= @setting.key %>
    </p>

    <p>
      <b>Value:</b>
      <%= @setting.value %>
    </p>

    
    <%= link_to '<< Back', admin_settings_path, :class => "btn btn-default back" %>
    <% if current_user.is_admin? %>
      <%= link_to 'Edit', edit_admin_setting_path(@setting), :class => "btn btn-default" %>
    <% end %>
  </div>
<% end %>


<script type="text/javascript">
  $(function(){
    App.initPageLayout(null, null, null, true);
  });
</script>