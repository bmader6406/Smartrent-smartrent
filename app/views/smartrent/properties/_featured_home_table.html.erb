<table class="homesTable">
  <tr>
    <td class="name"><%= home.name %></td>
    <td class="info"><%= home.beds %> Bedrooms / <%= home.baths %> Baths / <%= home.sq_ft %> sq. ft.</td>
  </tr>
  <tr>
    <td colspan="3">
      <div class="floorPlanImages">
        <% home.floor_plan_images.each do |floor_plan_image| %>
          <%= render partial: "home_floor_plan_row", :locals => {:floor_plan_image => floor_plan_image} %>
        <% end %>
      </div>
    </td>
  </tr>
</table>
