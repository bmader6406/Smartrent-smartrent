  font-size:3.6em;
  font-weight:400;
  margin-left:272px;
  width:707px;
  color:#7395bc;
  padding:34px 0 10px 0;
}

#contentWrapper #slides,
#contentWrapper #homeContent{
  width: 607px;
  position: relative;
  left: 274px;
  margin:20px 0 0 0;
}

#contentWrapper #homeContent{
  font-size:1em;
}

#contentWrapper #homeContent a{
  color:#000000;
}

#contentWrapper #slides .slides_container{
  width:607px;
  height:146px;
}

#slides .threeWide{
  width:610px;
}

#slides .threeWide .propertySlide{
  float:left;
  width:194px;
  margin:0 0 0 12px;
  overflow:hidden;
  position:relative;
  height:146px;
}

#slides .threeWide .propertySlide:first-child{
  margin:0;
}

#slides .threeWide .propertySlide img{
  width:194px;
  height:auto;
}

#slides .threeWide .propertySlide .caption{
  position:absolute;
  bottom:-55px;
  color:#000;
  background:url(<%= asset_path "carousel-caption-bg.png"%>);
  height:70px;
  padding: 10px 12px;
  font-size:0.8125em;
}

#slides .threeWide .propertySlide .caption strong{
  display: block;
  margin-bottom: 8px;
  padding-right: 15px;
}

#slides .threeWide .propertySlide .caption:after{
  color: #6b6985;
  content: "+";
  font-size: 1.384em;
  font-weight: normal;
  position: absolute;
  right: 10px;
  top: 7px;
}

#slides .next{
  position:absolute;
  bottom:0px;
  right:0px;
  background:url(<%= asset_path "slide_next.jpg"%>) no-repeat;
  width:36px;
  height:30px;
  font-size: 0;
  text-indent: -999em;
}

#slides .prev{
  position:absolute;
  bottom:0px;
  left:0px;
  background:url(<%= asset_path "slide_prev.jpg"%>) no-repeat;
  width:36px;
  height:30px;
  font-size: 0;
  text-indent: -999em;
  display:none;
}

#slides .pagination{
  height:30px;
  list-style:none;
  background:url(<%= asset_path "pagination_background.jpg"%>) repeat-x;
  text-align:center;
}

#slides .pagination li{
  background-color:#FFF;
  display:inline-block;
  padding:7px 20px 0 0;
  margin:1px 0 0 0;
  font-size:0.75em;
}

#slides .pagination li:first-child{
  padding:7px 10px 0 20px;
}

#slides .pagination li a,
#slides .pagination li a:visited{
  color:#000;
  text-decoration:none;
  outline:none;
}

#slides .pagination li.current a{
  font-weight:bold;
  outline:none;
}

