.modern-checkbox li label{
  position: relative;
  cursor: pointer;
  width: 100%;
  padding-left: 24px;
  -webkit-touch-callout: none;
  -webkit-user-select: none;
  -khtml-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
}
.modern-checkbox li label:before, .modern-checkbox li label:after {
  position: absolute; 
  top: 0;
  left: 0;
}
.modern-checkbox li label:before {
  content: '';
  width: 14px;
  height: 14px;
  border: 1px solid #C2C6CA;
  border-radius: 3px;
  margin: 2px 0;
}
.modern-checkbox li label:after {
  content:"";
  background: url(<%= asset_path "checkbox-large-dark.png"%>) no-repeat;
  background-size: contain;
  opacity: 0;
  transition: 0.2s;
  width: 18px;
  height: 18px;
  left: 1px;
}
.modern-checkbox li input[type="checkbox"] {
  display: none;
}
.modern-checkbox li input[type="checkbox"]:checked + label:after {
  max-width: 20px;
  opacity: 1;
}

