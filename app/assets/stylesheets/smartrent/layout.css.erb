@font-face{
  font-family:'FontSite Sans';
  src:url(<%= font_path "FontSiteSans-LightCd-webfont.eot" %>);
  src:local('?'),url(<%= font_path "FontSiteSans-LightCd-webfont.woff" %>) format('woff'),
      url(<%= font_path "FontSiteSans-LightCd-webfont.ttf" %>) format('truetype'),
      url('<%= font_path "FontSiteSans-LightCd-webfont.svg#webfont2X6HCJ0U" %>') format('svg');
      font-weight:normal;font-style:normal}

img{
  -ms-interpolation-mode:bicubic;
}

body{
  margin:0;
  padding:0;
  font-size:100%;
  font-family:'FontSite Sans','Arial Narrow',Arial,sans-serif;
}

p{
  font-family:'FontSite Sans','Arial Narrow',Arial,sans-serif;
}

a{
  text-decoration: underline;
}

label{
  font-weight: normal;
  margin-bottom: 0;
}

.bold{
  font-weight:bold;
}

.clear{
  clear:both;
}

.header{
  width:100%;
  border-bottom:1px solid #000000;
}

.header .fixedWidth{
  max-width:970px;
  height:260px;
  margin:0px auto 0px auto;
  position:relative;
}

.header .fixedWidth .logo{
  width:155px;
  padding:25px 30px 0 0;
  float:left;
  text-align:right;
}

.header .fixedWidth .menu{
  float:left;
  padding-top:20px;
  width:436px;
  text-transform:uppercase;
  /*
  font-family:"nimbus-sans-condensed",Arial,Helvetica Neue,Helvetica,sans-serif;
  font-size:2.95em;
  font-weight:300;
  */
  color:#ff9000;
  font-family:'FontSite Sans','Arial Narrow',Arial,sans-serif;
  font-size:60px;
  font-weight:500;
  margin-bottom:1px;
  
}

.header .fixedWidth .menu a{
  color:#c9c9c9;
  text-decoration:none;
}

.header .fixedWidth .menu .current_page_item{
  color:#2e2e39;
}

.header .fixedWidth .menu .current_page_item a{
  color:#2e2e39;
}

.header .fixedWidth .menu a:hover{
  color:#2e2e39;
}

.header .fixedWidth .menu ul{
  list-style:none;
  margin:0px;
  padding:0px;
}

.header .fixedWidth .menu ul li{
  margin:0px;
  padding:0px;
  line-height:.75em;
}

.header .fixedWidth .searchOptions{
  float:left;
}

.header .fixedWidth .searchOptions .contactLink{
  width:215px;
  font-size:.7em;
  color:#636972;
  text-align:right;
  text-transform:uppercase;
  margin-top:10px;
}

.header .fixedWidth .searchOptions .contactLink a{
  color:#636972;
  text-decoration:none;
}

.header .fixedWidth .searchOptions .contactLink a:hover{
  color:#2e2e39;
}

.header .fixedWidth .searchOptions #special-nav{
  position:absolute;
  bottom:0;
  right:0;
  width:240px;
  list-style:none;
  margin:0;
  padding:0;
}

.header .fixedWidth .searchOptions #special-nav a,
.header .fixedWidth .searchOptions #special-nav a:visited{
  background:#dde0e3 url(<%= asset_path "bg-special-nav.gif"%>) no-repeat top right;
  font-size:15px;
  line-height:26px;
  color:#000;
  text-decoration:none;
}
.header .fixedWidth .searchOptions #special-nav .arrow a{
  background-image:url(<%= asset_path "bg-special-nav-arrow.png"%>)
}
.header .fixedWidth .searchOptions #special-nav a:hover,#special-nav li:hover a{
  background-color:#c2c6ca;
  text-decoration:none;
}

.header .fixedWidth .searchOptions #special-nav li{
  position:relative;
  width:230px;
  border-top:1px solid #fff;
}

.header .fixedWidth .searchOptions #special-nav li a{
  display:block;
  width:210px;
  padding:0 10px;
}

.banner{
  width:100%;
}

.dropshadow{
  background:url("<%= asset_path "bannerGradient.gif"%>") bottom left repeat-x;
  padding-bottom:3px;
}

.banner .fixedWidth,
#contentWrapper .fixedWidth{
  max-width:1100px;
  width:100%;
